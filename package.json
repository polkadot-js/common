{
  "name": "@polkadot/util",
  "version": "0.5.1",
  "description": "A collection of useful utilities for @polkadot",
  "main": "index.js",
  "keywords": [
    "Polkadot"
  ],
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": ">=6.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polkadot-js/util.git"
  },
  "bugs": {
    "url": "https://github.com/polkadot-js/util/issues"
  },
  "homepage": "https://github.com/polkadot-js/util#readme",
  "scripts": {
    "build": "npm run build:js && npm run build:md",
    "build:js": "rimraf build && babel src --out-dir build --ignore *.spec.js",
    "build:md": "polkadot-dev-build-docs",
    "check": "npm run check:lint && npm run check:flow",
    "check:flow": "flow check",
    "check:lint": "eslint src",
    "test": "cross-env NODE_ENV=test babel-istanbul cover _mocha 'src/*.spec.js' 'src/**/*.spec.js'"
  },
  "devDependencies": {
    "@polkadot/dev": "^0.1.7",
    "@polkadot/jsonrpc": "^0.3.5"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bn.js": "^4.11.8",
    "keccak": "^1.3.0"
  }
}
